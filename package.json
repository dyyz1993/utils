{
  "name": "@dyyz1993/utils",
  "version": "0.0.2",
  "description": "gen doc by utils",
  "scripts": {
    "test": "mocha 'test/**/*.ts'",
    "gen-test": "ts-node lib/gen-test.ts",
    "gen-docs": "ts-node lib/gen-docs.ts",
    "gen-index": "ts-node lib/gen-index.ts",
    "build": "npm run gen-docs && npm run gen-index && tsc",
    "build:browser": "npm run bundle",
    "bundle": "rollup --config",
    "publish": "npm config set registry=http://registry.npmjs.org && npm publish --access public && npm config set registry=https://registry.npm.taobao.org"
  },
  "module": "dist/all.js",
  "files": [
    "dist",
    "output"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "xuyingzhou",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@supabase/doctest-js": "^0.1.0",
    "@types/chai": "^4.2.18",
    "@types/ejs": "^3.0.6",
    "@types/fs-extra": "^9.0.11",
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/structured-source": "^3.0.0",
    "@types/url-parse": "^1.4.3",
    "chai": "^4.3.4",
    "comment-parser": "^1.1.5",
    "doctrine": "^3.0.0",
    "ejs": "^3.1.6",
    "fs-extra": "^10.0.0",
    "get-function-location": "^2.0.0",
    "glob": "^7.1.7",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^8.4.0",
    "parse-comments": "^1.0.0",
    "rollup": "^2.51.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-rename-node-modules": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "structured-source": "^3.0.2",
    "ts-node": "^10.0.0",
    "ts-node-test-register": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "query-string": "^7.0.0",
    "url": "^0.11.0",
    "url-parse": "^1.5.1"
  }
}
